{
    // Old Version
    "mc-color.enabled": true,
    "mc-color.markerType": "foreground",
    "mc-color.languages": ["*"],
    "mc-color.newLineDelimiter": true,
    "mc-color.delimiters": ["`", "\"", "'"],
    "mc-color.prefixes": ["ยง"],
    "mc-color.mode": "bedrock",
    "mc-color.replicateJavaBug": false,
    // Pre Release Verison
    "mc-color.enable": true,
    "mc-color.version": "bedrock",
    "mc-color.marker": "foreground",
    "mc-color.fallback": true,
    "mc-color.fallbackRegex": [
        "(?<!\\\\)\"", // Unescaped double quotes
        "(?<!\\\\)'", // Unescaped single quotes
        "(?<!\\\\)`", // Unescaped backticks
        "\\r?\\n" // Newline (Unix or Windows style)
    ],
    "editor.formatOnSave": true,
    "editor.defaultFormatter": "dbaeumer.vscode-eslint",
    "eslint.format.enable": true,
    "editor.insertSpaces": true,
    "editor.tabSize": 4,
    "files.eol": "\n",
    "files.exclude": {
        "**/.git": true,
        "**/.svn": true,
        "**/.hg": true,
        "**/.DS_Store": true,
        "**/Thumbs.db": true
    },
    "hide-files.files": []
}
